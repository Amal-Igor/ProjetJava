package fr.dawan.tableaux.tableauxdynamiques;

import java.util.ArrayList;

public class Wrapper {

	public static void main(String[] args) {
		ArrayList<Integer> tab = new ArrayList();
		
		//Parser une chaine de caractère entier
		int k = Integer.parseInt("20");
		
		//On peut aussi convertir une chaine de caractère en un entier de type Wrapper
		Integer i = Integer.valueOf("210");
		System.out.println(i);
		
		//Convertir une chaien de caractère en double
		Double d = Double.valueOf("35.14");
		
		//Les wrappers sont des objets des classes de type complexe ==> on peut appliquer des méthodes
		//Tableau dynamique == collections
		
		System.out.println(d);
	}

}
